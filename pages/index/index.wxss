}
.page3 button {
  line-height: 2!important;
}
.page3 .allLast {
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #999;
  font-size: 28rpx;
}
.page3 .price {
  font-size: 40rpx;
}
.page3 .allLast navigator {
  color: #333;
  width: 140rpx;
  text-align: center;
  display: inline-block;
  border: 2rpx #ddd solid;
  padding: 14rpx 10rpx;
  border-radius: 6rpx;
}
.page3 .allLast .bg_y {
  background: #ffe400;
  border: 2rpx #ffe400 solid;
  margin-left: 20rpx;
}
.page3 .orderOk {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 9999999999;
  background: rgba(0,0,0,.2);
}
.page3 .okMessage {
  width: 90%;
  background: white;
  border-radius: 12rpx;
  margin-left: 5%;
