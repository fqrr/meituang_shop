}
.page3 .tip,.shouhuo {
  padding: 80rpx 0 0;
  text-align: center;
}
.shouhuo {
  padding: 40rpx 0 80rpx;
}
.yesorno {
  display: flex;
  justify-content: space-between;
  border-top: 2rpx #ddd solid;
}
.okCancel,.okOk {
  width: 50%;
  padding: 30rpx 0;
}
.okOk {
  border-left: 2rpx #ddd solid;
  background: #333;
  color: white;
  border-radius: 0 0 12rpx 0;
}
.page3 .order_name {
  display: inline-block;
  width: 400rpx;
}
